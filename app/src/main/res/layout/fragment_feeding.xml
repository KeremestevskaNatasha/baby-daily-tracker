<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:orientation="vertical"
	android:background="@color/colorPrimary"
	android:weightSum="5"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:id="@+id/listNutritions">
	
	<RelativeLayout
		android:layout_weight="1"
		android:layout_width="match_parent"
		android:layout_height="0dp">
		
		<TextView
			android:id="@+id/textGrid"
			android:text="@string/table_for_nutritions"
			android:textSize="30sp"
			android:textColor="#000"
			android:layout_centerInParent="true"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
	
	</RelativeLayout>
	
	<GridLayout
		android:id="@+id/mainGrid"
		android:rowCount="3"
		android:columnCount="2"
		android:alignmentMode="alignMargins"
		android:columnOrderPreserved="false"
		android:layout_weight="8"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:padding="14dp">
		
		<!-- ROW 1  -->
		<!-- COLUMN 1  -->
		
		
		
		<androidx.cardview.widget.CardView
			android:id="@+id/months6"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_columnWeight="1"
			android:layout_marginBottom="16dp"
			android:layout_rowWeight="1"
			android:layout_marginLeft="16dp"
			android:layout_marginRight="16dp">
			
			<LinearLayout
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_margin="16dp"
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				
				<TextView
					android:layout_gravity="left"
					android:text="6+ months"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					/>
			
			</LinearLayout>
		
		</androidx.cardview.widget.CardView>
		
		<!-- COLUMN 2  -->
		
		<androidx.cardview.widget.CardView
			android:id="@+id/months7"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_columnWeight="1"
			android:layout_marginBottom="16dp"
			android:layout_rowWeight="1"
			android:layout_marginLeft="16dp"
			android:layout_marginRight="16dp"
			app:cardElevation="8dp"
			app:cardCornerRadius="8dp">
			
			<LinearLayout
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_margin="16dp"
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				
				<TextView
					android:layout_gravity="left"
					android:text="7+ months"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			
			</LinearLayout>
		
		</androidx.cardview.widget.CardView>
		
		<!-- ROW 2  -->
		<!-- COLUMN 1  -->
		
		<androidx.cardview.widget.CardView
			android:id="@+id/months8"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_columnWeight="1"
			android:layout_marginBottom="16dp"
			android:layout_rowWeight="1"
			android:layout_marginLeft="16dp"
			android:layout_marginRight="16dp"
			app:cardElevation="8dp"
			app:cardCornerRadius="8dp">
			
			<LinearLayout
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_margin="16dp"
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				
				<TextView
					
					android:layout_gravity="left"
					android:text=" 8+ months"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
				
			
			</LinearLayout>
		
		</androidx.cardview.widget.CardView>
		
		<!-- COLUMN 2  -->
		
		<androidx.cardview.widget.CardView
			android:id="@+id/months9"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_columnWeight="1"
			android:layout_marginBottom="16dp"
			android:layout_rowWeight="1"
			android:layout_marginLeft="16dp"
			android:layout_marginRight="16dp"
			app:cardElevation="8dp"
			app:cardCornerRadius="8dp">
			
			<LinearLayout
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_margin="16dp"
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				
				<TextView
					android:layout_gravity="left"
					android:text="9+ months"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			
			</LinearLayout>
		
		</androidx.cardview.widget.CardView>
		
		
		<!-- ROW 3  -->
		<!-- COLUMN 1  -->
		
		<androidx.cardview.widget.CardView
			android:id="@+id/months10"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_columnWeight="1"
			android:layout_marginBottom="16dp"
			android:layout_rowWeight="1"
			android:layout_marginLeft="16dp"
			android:layout_marginRight="16dp"
			app:cardElevation="8dp"
			app:cardCornerRadius="8dp">
			
			<LinearLayout
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_margin="16dp"
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				
				<TextView
					android:layout_gravity="left"
					android:text="10+ months"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
				
			
			</LinearLayout>
		
		</androidx.cardview.widget.CardView>
		
		<!-- COLUMN 2  -->
		
		<androidx.cardview.widget.CardView
			android:id="@+id/months11"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_columnWeight="1"
			android:layout_marginBottom="16dp"
			android:layout_rowWeight="1"
			android:layout_marginLeft="16dp"
			android:layout_marginRight="16dp"
			app:cardElevation="8dp"
			app:cardCornerRadius="8dp">
			
			<LinearLayout
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_margin="16dp"
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				
				<TextView
					android:layout_gravity="left"
					android:text="11+ months"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
				
			
			</LinearLayout>
		
		</androidx.cardview.widget.CardView>
		
		<androidx.cardview.widget.CardView
			android:id="@+id/months12"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_columnWeight="1"
			android:layout_marginBottom="16dp"
			android:layout_rowWeight="1"
			android:layout_marginLeft="16dp"
			android:layout_marginRight="16dp"
			app:cardElevation="8dp"
			app:cardCornerRadius="8dp">
			
			<LinearLayout
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_margin="16dp"
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				
				<TextView
					android:layout_gravity="left"
					android:text="12+ months"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			
			
			</LinearLayout>
		
		</androidx.cardview.widget.CardView>
		
		
		<androidx.cardview.widget.CardView
			android:id="@+id/other"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:layout_columnWeight="1"
			android:layout_marginBottom="16dp"
			android:layout_rowWeight="1"
			android:layout_marginLeft="16dp"
			android:layout_marginRight="16dp"
			app:cardElevation="8dp"
			app:cardCornerRadius="8dp">
			
			<LinearLayout
				android:layout_gravity="center_horizontal|center_vertical"
				android:layout_margin="16dp"
				android:orientation="vertical"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				
				<TextView
					android:layout_gravity="left"
					android:text="Other.."
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			
			
			</LinearLayout>
		
		</androidx.cardview.widget.CardView>
	
	</GridLayout>
	
</LinearLayout>